  "model": "opus",
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "jq -r '.tool_input.command // empty' | grep -qE 'git push.*(-f|--force)' && { echo '{\"hookSpecificOutput\":{\"hookEventName\":\"PreToolUse\",\"permissionDecision\":\"ask\",\"permissionDecisionReason\":\"Force push detected\"}}'; exit 0; } || exit 0"
          },
          {
            "type": "command",
            "command": "jq -r '.tool_input.command // empty' | grep -qE 'git add (-A|--all|[.]$|[.] )' && echo '{\"hookSpecificOutput\":{\"hookEventName\":\"PreToolUse\",\"permissionDecision\":\"deny\",\"permissionDecisionReason\":\"一括追加は禁止です。ファイルを個別に確認し、必要なものだけ追加してください\"}}' || true"
          },
          {
            "type": "command",
            "command": "jq -r '.tool_input.command // empty' | grep -qE -- '--no-verify' && echo '{\"hookSpecificOutput\":{\"hookEventName\":\"PreToolUse\",\"permissionDecision\":\"ask\",\"permissionDecisionReason\":\"--no-verify でフックをスキップしようとしています\"}}' || true"
          },
          {
            "type": "command",
            "command": "jq -r '.tool_input.command // empty' | grep -qE 'git\\s+(-[a-zA-Z]+\\s+)*-C\\s' && echo '{\"hookSpecificOutput\":{\"hookEventName\":\"PreToolUse\",\"permissionDecision\":\"deny\",\"permissionDecisionReason\":\"git -C は禁止です。cd path && git ... の形式で実行してください\"}}' || true"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "jq -r '.tool_input.file_path | select(endswith(\".js\") or endswith(\".ts\") or endswith(\".jsx\") or endswith(\".tsx\"))' | xargs -r npx biome check --write --unsafe"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "jq -n --arg cwd \"$PWD\" '{notification_type:\"stop\",cwd:$cwd}' | ~/.claude/hooks/claude-notification.sh"
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "~/.claude/hooks/claude-notification.sh"
          }
        ]
      }
    ],
    "PermissionRequest": [
      {
        "matcher": "ExitPlanMode",
        "hooks": [
          {
            "type": "command",
            "command": "jq -r '.tool_input.plan // empty' | pbcopy"
          }
        ]
      }
    ],
    "WorktreeCreate": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash -c 'NAME=$(jq -r .name); OUT=$(wtp add -b \"$NAME\" 2>&1); echo \"$OUT\" >&2; echo \"$OUT\" | grep \"Location:\" | sed \"s/.*Location: //\"'"
          }
        ]
      }
    ],
    "WorktreeRemove": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash -c 'WT_PATH=$(jq -r .worktree_path); NAME=$(basename \"$WT_PATH\"); wtp rm --force \"$NAME\" >&2 2>&1'"
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "input=$(cat); model=$(echo \"$input\" | jq -r '.model.display_name // \"Claude\"'); style=$(echo \"$input\" | jq -r '.output_style.name // \"default\"'); cwd=$(echo \"$input\" | jq -r '.workspace.current_dir'); project=$(echo \"$input\" | jq -r '.workspace.project_dir'); if [ \"$cwd\" != \"$project\" ] && [[ \"$cwd\" == \"$project\"/* ]]; then rel_path=\"$(basename \"$project\")/$(echo \"$cwd\" | sed \"s|^$project/||\")\"; else rel_path=\"$(basename \"$cwd\")\"; fi; if git -C \"$cwd\" rev-parse --is-inside-work-tree >/dev/null 2>&1; then branch=$(git -C \"$cwd\" branch --show-current 2>/dev/null || echo \"detached\"); printf \"\\033[2m%s\\033[0m \\033[36m%s\\033[0m \\033[2m|\\033[0m \\033[33m%s\\033[0m \\033[2m|\\033[0m \\033[35m%s\\033[0m\" \"$model\" \"$rel_path\" \"$branch\" \"$style\"; else printf \"\\033[2m%s\\033[0m \\033[36m%s\\033[0m \\033[2m|\\033[0m \\033[35m%s\\033[0m\" \"$model\" \"$rel_path\" \"$style\"; fi"
  },
